

<routes>
   <vType id="car"
        vClass="passenger" length="5" minGap="1" accel="3.5" decel="2.2" tau="1.3"
        maxSpeed="20" carFollowModel="IDM" laneChangeModel="LC2013"
    />

    <vType id="slowcar"
       speedFactor="norm(0.2,0.01)" 
    />

    <route id="route_0" edges="E0 E1" />
    <route id="route_1" edges="R0 E1" />

    <flow id="carflow1" type="car" begin="0" end="60" vehsPerHour="5000" from="E0" to="E1" departLane="random" departSpeed="speedLimit">
        <randomRoute probability="1" />
    </flow>

    <flow id="carflow2" color="1,0,0" type="slowcar" begin="0" end="60" vehsPerHour="3000" route="route_0" departLane="random" departSpeed="random">
        <randomRoute probability="1" />
    </flow>

    <flow id="carflow3" color="1,1,0" type="car" begin="0" end="60" vehsPerHour="1000" route="route_1" departSpeed="random">
        <randomRoute probability="1" />
    </flow>
    <!-- <vehicle id="0" type="car" route="route_1" depart="0"/> -->

</routes>

